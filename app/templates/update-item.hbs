{{!-- {{#each model as |item|}} --}}
<p>The name of this model is "{{model.name}}"</p>
<form {{action 'editItem' model on='submit'}}>
  {{input value=model.name}}
  {{input value=model.description}}
  {{input value=model.price}}
  {{input value=model.category}}
  <button class="btn btn-primary"> Save Changes </button>
</form>
<br />
{{!-- {{/each}} --}}

{{outlet}}
